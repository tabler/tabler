{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": false
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ],
      "cache": false
    },
    "bundlewatch": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}